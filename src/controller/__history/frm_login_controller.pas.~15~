unit frm_login_controller;


interface
uses my_contracts;

implementation

type
  TLoginController = class(TInterfacedObject,IControllerLogin)


  private
    FView : ILoginView;
    function ProcessarNome : String;
  public
    constructor Create(const aView: ILoginView);

  end;

{ TLoginController }

constructor TLoginController.Create(const aView: ILoginView);
begin
  FView := aView;
end;

function TLoginController.ProcessarNome: String;
begin
  var a := Fview.GetNome;
  Result := a + ' processado com sucesso';
end;

end.
