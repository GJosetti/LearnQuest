unit frm_login_controller;


interface
uses Vcl.Dialogs,my_contracts;

implementation

type
  TLoginController = class(TInterfacedObject,ILoginController)
  private
    FView : ILoginView;
    procedure ProcessarNome;
  public
    constructor Create(const aController: TLoginController);

  end;

{ TLoginController }

constructor TLoginController.Create();
begin

end;

procedure TLoginController.ProcessarNome;
begin
  var a := Fview.GetNome;
  ShowMessage( a + ' processado com sucesso');

  end;


end.
